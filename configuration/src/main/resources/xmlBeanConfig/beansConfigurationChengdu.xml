<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="chengDu" class="com.example.configuration.City">
        <property name="name" value="xml bean chengdu" />
        <property name="population" value="16000000" />
        <property name="area" value="1.46万平方公里" />
    </bean>

    <bean id="zhangSan" class="com.example.configuration.People">
        <property name="name" value="xml bean zhangsan" />
        <property name="age" value="16" />
    </bean>

</beans>